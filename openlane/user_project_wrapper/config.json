{
    "//": "Design files",
    "VERILOG_FILES": [
        "dir::../../verilog/rtl/defines.v",
        "dir::../../verilog/rtl/user_project_wrapper.v"
    ],
    "PNR_SDC_FILE": "dir::signoff.sdc",
    
    "//": "Hardening strategy variables (this is for 1-Macro-First Hardening). Visit https://docs.google.com/document/d/1pf-wbpgjeNEM-1TcvX2OJTkHjqH_C9p-LURCASS0Zo8 for more info",
    "SYNTH_ELABORATE_ONLY": true,
    "RUN_POST_GPL_DESIGN_REPAIR": false,
    "RUN_POST_CTS_RESIZER_TIMING": false,
    "DESIGN_REPAIR_BUFFER_INPUT_PORTS": false,
    "FP_PDN_ENABLE_RAILS": false,
    "RUN_LINTER": false,
    "RUN_ANTENNA_REPAIR": false,
    "RUN_FILL_INSERTION": false,
    "RUN_TAP_ENDCAP_INSERTION": false,
    "RUN_CTS": false,
    "RUN_IRDROP_REPORT": false,
    "ERROR_ON_SYNTH_CHECKS": false,

    "IGNORE_DISCONNECTED_MODULES": ["u_picorv32"],

    "//": "Macros configurations",
    "MACROS": {
        "picorv32": {
            "gds": [
                "dir::../../gds/picorv32.gds"
            ],
            "lef": [
                "dir::../../lef/picorv32.lef"
            ],
            "//instances": {
                "u_picorv32": {
                    "location": [300, 100],
                    "orientation": "N"
                }
            },
            "nl": [
                "dir::../../verilog/gl/picorv32.v"
            ],
            "spef": {
                "min_*": [
                    "dir::../../spef/multicorner/picorv32.min.spef"
                ],
                "nom_*": [
                    "dir::../../spef/multicorner/picorv32.nom.spef"
                ],
                "max_*": [
                    "dir::../../spef/multicorner/picorv32.max.spef"
                ]
            },
            "lib": {
                "*": "dir::../../lib/picorv32.lib"
            }
        },
        "DFFRAM512x32": {
            "gds": [
                "dir::../../ip/DFFRAM512x32/layout/gds/DFFRAM512x32.gds.gz"
            ],
            "lef": [
                "dir::../../ip/DFFRAM512x32/layout/lef/DFFRAM512x32.lef"
            ],
            "instances": {
                "DFFRAM": {
                    "location": [1200, 800],
                    "orientation": "E"
                }
            },
            "nl": [
                "dir::../../ip/DFFRAM512x32/hdl/gl/DFFRAM512x32.v"
            ],
            "spef": {
                "min_*": [
                    "dir::../../ip/DFFRAM512x32/timing/spef/DFFRAM512x32.min.spef"
                ],
                "nom_*": [
                    "dir::../../ip/DFFRAM512x32/timing/spef/DFFRAM512x32.nom.spef"
                ],
                "max_*": [
                    "dir::../../ip/DFFRAM512x32/timing/spef/DFFRAM512x32.max.spef"
                ]
            },
            "lib": {
                "*": "dir::../../ip/DFFRAM512x32/timing/lib/nom/DFFRAM512x32.Typical.lib"
            }
        }
    },
    "ERROR_ON_DISCONNECTED_PINS": false,
    "PDN_MACRO_CONNECTIONS": ["u_picorv32 vccd1 vssd1 vccd1 vssd1", "DFFRAM vccd1 vssd1 VPWR VGND"],

    "//": "PDN configurations",
    "FP_PDN_VOFFSET": 5,
    "FP_PDN_HOFFSET": 5,
    "FP_PDN_VWIDTH": 3.1,
    "FP_PDN_HWIDTH": 3.1,
    "FP_PDN_VSPACING": 15.5,
    "FP_PDN_HSPACING": 15.5,
    "FP_PDN_VPITCH": 180,
    "FP_PDN_HPITCH": 180,
    "ERROR_ON_PDN_VIOLATIONS": false,

    "//": "Magic variables",
    "MAGIC_DRC_USE_GDS": true,

    "DRT_THREADS": 1,
    "MAX_TRANSITION_CONSTRAINT": 1.5,
    "MAX_FANOUT_CONSTRAINT": 35,
    
    "//": "Fixed configurations for caravel. You should NOT edit this section",
    "DESIGN_NAME": "user_project_wrapper",
    "FP_SIZING": "absolute",
    "DIE_AREA": [0, 0, 2920, 3520],
    "FP_DEF_TEMPLATE": "dir::fixed_dont_change/user_project_wrapper.def",
    "VDD_NETS": [
        "vccd1",
        "vccd2",
        "vdda1",
        "vdda2"
    ],
    "GND_NETS": [
        "vssd1",
        "vssd2",
        "vssa1",
        "vssa2"
    ],
    "FP_PDN_CORE_RING": true,
    "FP_PDN_CORE_RING_VWIDTH": 3.1,
    "FP_PDN_CORE_RING_HWIDTH": 3.1,
    "FP_PDN_CORE_RING_VOFFSET": 12.45,
    "FP_PDN_CORE_RING_HOFFSET": 12.45,
    "FP_PDN_CORE_RING_VSPACING": 1.7,
    "FP_PDN_CORE_RING_HSPACING": 1.7,
    "CLOCK_PORT": "wb_clk_i",
    "SIGNOFF_SDC_FILE": "dir::signoff.sdc",
    "MAGIC_DEF_LABELS": false,
    "CLOCK_PERIOD": 25,
    "MAGIC_ZEROIZE_ORIGIN": false,
    "meta": {
        "version": 2
    }
}
